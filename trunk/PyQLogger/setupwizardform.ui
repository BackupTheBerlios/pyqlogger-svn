        <slot>editLogin_textChanged(const QString&amp;)</slot>
    </connection>
    <connection>
        <sender>comboBlogs</sender>
        <signal>activated(int)</signal>
        <receiver>SetupWizardForm</receiver>
        <slot>comboBlogs_activated(int)</slot>
    </connection>
    <connection>
        <sender>btnFetchBlogs</sender>
        <signal>clicked()</signal>
        <receiver>SetupWizardForm</receiver>
        <slot>btnFetchBlogs_clicked()</slot>
    </connection>
    <connection>
        <sender>SetupWizardForm</sender>
        <signal>selected(const QString&amp;)</signal>
